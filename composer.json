{
    "name": "wuori/feature-creature",
    "description": "A simple coverage reporting suite for feature tests in Laravel.",
    "keywords": [
        "laravel"
        "testing"
        "coverage"
    ],
    "homepage": "https://github.com/wuori/feature-creature.git",
    "authors": [
        {
            "name": "Michael Wuori",
            "email": "michael.wuori@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "5.5.*|5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.*|7.0.*",
        "orchestra/testbench": "3.5.*|3.6.*"
    },
    "autoload": {
       "psr-4": {
            "Vendor\\Package\\": "src/FeatureCreature"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\Package\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\Package\\ServiceProvider"
            ]
        }
    },
    "license": "MIT"
    
}
